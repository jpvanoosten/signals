@ECHO OFF

PUSHD %~dp0

SET CMAKE=cmake
SET CMAKE_GENERATOR="Visual Studio 16 2019"
SET CMAKE_BINARY_DIR=build_vs2019

MKDIR %CMAKE_BINARY_DIR% 2>NUL
PUSHD %CMAKE_BINARY_DIR%

%CMAKE% -G %CMAKE_GENERATOR% -Wno-dev "%~dp0"

IF %ERRORLEVEL% NEQ 0 (
    PAUSE
) ELSE (
    START signals.sln
)

POPD

:Exit

POPD

